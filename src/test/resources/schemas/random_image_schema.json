{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "message",
    "status"
  ],
  "properties": {
    "status": {
      "type": "string",
      "enum": [
        "success"
      ]
    },
    "message": {
      "type": "string",
      "pattern": "^https?://.+"
    }
  },
  "additionalProperties": true
}